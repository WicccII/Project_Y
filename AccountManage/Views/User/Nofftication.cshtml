@model AccountManage.Models.User
@{
    ViewData["Title"] = "Nofftication";
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<h1>Valid OTP click submit to back home!!!</h1>
<form method="post" asp-action="Nofftication">
    <div class="form-group">
        <input readonly type="hidden" asp-for="Id" class="form-control" />
    </div>
<div class="form-group">
        <input readonly type="hidden" asp-for="FirstName" class="form-control" />
        <span asp-validation-for="FirstName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="LastName" class="form-control" />
        <span asp-validation-for="LastName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <div class="form-group">
            <input readonly type="hidden" asp-for="DateOfBirth" class="form-control" />
        </div>
        <span asp-validation-for="DateOfBirth" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="Gender" class="form-control" />
        <span asp-validation-for="Gender" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="Address" class="form-control" />
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="ZipCode" class="form-control" />
        <span asp-validation-for="ZipCode" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="PhoneNumber" class="form-control" />
        <span asp-validation-for="PhoneNumber" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="Email" class="form-control" />
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="Username" class="form-control" />
        <span asp-validation-for="Username" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="IdCardPassportNumber" class="form-control" />
        <span asp-validation-for="IdCardPassportNumber" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="PasswordHash" class="form-control" />
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="RoleType" class="form-control" />
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="IsDeleted" class="form-control" />
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="IsEmail" class="form-control " value="true" />
    </div>
    <div class="form-group">
        <input readonly type="hidden" asp-for="OTP" class="form-control " value="" />
    </div>
    
    <button type="submit">Submit</button>
</form>